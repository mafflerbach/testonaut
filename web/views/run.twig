{% include 'head.twig' %}
<div class="contentBox">
  <div class="content">
    <a href="{{ app.request.baseUrl }}/">root</a>

    {% for results in app.result %}
      {% for result in results %}
        <table class="table-bordered">
          {% for resultEntry in result %}
            {% if resultEntry.0  == true %}
              {% set class = 'bg-success' %}
            {% else %}
              {% set class = 'bg-danger' %}
            {% endif %}
            <tr class="{{ class }}">
              <td>Command</td>
              <td>{{  resultEntry.2}}</td>
              <td>{{  resultEntry.1}}</td>            
            </tr>
          {% endfor %}
        </table>
      {% endfor %}
    {% endfor %}

    <a href="{{ app.request.baseUrl }}/{{ app.request.path }}"
       class="backLink btn">Zur√ºck</a>
  </div>
</div>
{% include 'footer.twig' %}